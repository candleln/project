<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>자료실</title>
<style type="text/css">
#top {
	padding-top: 10px;
	background-color: YELLOW;
	text-align: center;
	height: 100px;
}

#left {
	padding-top: 20px;
	background-color: BLUE;
	text-align: center;
	float: left;
	width: 100px;
	height: 100%;
}

#container {
	float: none;
	margin-left: 30%;
	margin-right: 30%
}

#bottom {
	float: right;
}

table {
	border-collapse: collapse;
	width: 800px;
}

th, td {
	padding: 10px;
	border-bottom: 1px solid BLACK;
}
</style>
</head>
<body>
	<div>
		<div id="top">
			<h1>자료실</h1>
		</div>
		<div id="left">
			<a href="">*목록</a><br>
			<a href="">*목록</a><br>
		</div>
		<div>
			<div id="container">
				<form action="" method="post">
					<table border="1" style="border-style: ridge">
						<tr>
							<th>파일명</th>
							<th><input type="file" name=""></th>
						</tr>
						<tr>
							<th>OS</th>
							<th>Windows</th>
						</tr>
						<tr>
							<th>분석정보</th>
							<th><textarea rows="" cols=""></textarea></th>
						</tr>
						<tr>
							<th>업데이트 날짜</th>
							<th><input type="date"></th>
						</tr>
					</table>
					<div id="bottom">
						<button type="submit">입력완료</button>
						<button onclick="">취소</button>
					</div>
				</form>
			</div>
		</div>
	</div>
</body>
</html>